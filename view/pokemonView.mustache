<div class="w3-container w3-content w3-center w3-padding-64" style="max-width:800px" id="band">
    <h2 class="w3-wide">Pokemon</h2>
    <table class="w3-table w3-striped w3-border">
        <tr>
            <th>ID</th>
            <th>Nombre</th>
            <th>Tipo</th>
            <th>Imagen</th>
        </tr>
        {{#pokemon}}
            <tr>
                <td>{{numero_identificador}}</td>
                <td>{{nombre}}</td>
                <td><img src="public/imagen/tipo/{{tipo}}.webp" alt="" width="100em" height="100em"></td>

                <td><img src={{imagen}} alt="" width="100em" height="100em"></td>

                <td>{{descripcion}}</td>

            </tr>
        {{/pokemon}}
    </table>
</div>

